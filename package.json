{
  "name": "playit.gg",
  "version": "1.3.9",
  "description": "An Unofficial JavaScript Wrapper For https://playit.gg",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "tsc",
    "prepare": "tsc",
    "package": "babel dist -d pkg/ --presets es2015 --copy-files && pkg . ",
    "test": "tsc && node dist/test.js"
  },
  "bin": {
    "playit": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheBotlyNoob/playit.gg.git"
  },
  "keywords": [
    "playit",
    "gaming"
  ],
  "author": "Jay Jackson",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16.7.1",
    "@types/node-fetch": "^2.5.12",
    "commander": "^8.1.0",
    "exit-hook": "^2.2.1",
    "nanoid": "^3.1.25",
    "node": "^16.6.1",
    "playit.gg": "^1.3.8",
    "typescript": "^4.3.5"
  },
  "pkg": {
    "scripts": "pkg/**/*",
    "targets": [
      "node14-linux"
    ],
    "outputPath": "bin"
  }
}
